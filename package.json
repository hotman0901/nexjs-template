  "description": "nextjs-typescript-i18n-prettier-eslint-redux-observable-styled-component",
  "main": "server.js",
  "scripts": {
    "dev": "env-cmd -f .env.stage next -p 6100",
    "build": "next build",
    "build:stage": "env-cmd -f .env.stage next build",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:prod": "env-cmd -f .env.prod next build",
    "start": "next start -p 6100",
    "check-format": "prettier --check .",
    "lint": "npm run check-format && next lint && eslint src pages -c .eslintrc.json --ext js,jsx",
    "precommit": "echo 'pre-commit checks...' && exit 0",
    "fix-code": "eslint src pages -c .eslintrc.json --fix --ext js,jsx",
    "build-docker-env": "next build",
    "start-docker-env": "next start -p 6100",
    "release": "standard-version",
    "release-tag": "git push --follow-tags origin master"
  },
